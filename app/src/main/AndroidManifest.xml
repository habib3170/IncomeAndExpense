<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.habib4990gmail.incomeandexpensemanagement">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="Expense Managment">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddincomeActivity"
            android:label="Add Income"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AddexpenseActivity"
            android:label="Add Expense"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".IncomelistActivity"
            android:label="Income List"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".ExpenselistActivity"
            android:label="Expense List"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>